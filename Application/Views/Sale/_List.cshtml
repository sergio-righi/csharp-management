@model ICollection<Domain.Entity.Specific.SProduct>

@if (!Model.IsNullOrEmpty())
{
    <ul class="list list-border">

        @for (var i = 0; i < Model.Count; i++)
        {
            await Html.RenderPartialAsync("~/Views/Sale/_Product.cshtml", Model.ElementAt(i));
        }

    </ul>

    <br />
}
else
{
    @Html.Feedback(EFeedback.Info, Message.NothingToList, false)
}